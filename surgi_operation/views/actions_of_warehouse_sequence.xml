<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!--generate sequence number for back to warehouse action-->
        <record id="seq_id_back_to_warehouse_request" model="ir.sequence">
            <field name="name">back_to_warehouse</field>
            <field name="code">back_to_warehouse_number</field>
            <field name="prefix">/back</field>
            <field name="padding">5</field>
        </record>


        <!--generate sequence number for move to another operation action-->
        <record id="seq_id_move_to_operation_request" model="ir.sequence">
            <field name="name">move_to_operation</field>
            <field name="code">move_to_operation_number</field>
            <field name="prefix">/move</field>
            <field name="padding">5</field>
        </record>

        <!--generate sequence number for move to hanged warehouse action-->
        <record id="seq_id_move_to_hanged_warehouse_request" model="ir.sequence">
            <field name="name">move_to_hanged_warehouse</field>
            <field name="code">move_to_hanged_warehouse_number</field>
            <field name="prefix">/move</field>
            <field name="padding">5</field>
        </record>

        <!--generate sequence number for move to hanged warehouse action-->
        <record id="seq_id_hanged_back_to_warehouse_request" model="ir.sequence">
            <field name="name">hanged_back_to_warehouse</field>
            <field name="code">hanged_back_to_warehouse_number</field>
            <field name="prefix">/move</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>