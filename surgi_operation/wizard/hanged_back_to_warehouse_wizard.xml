<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="wizard_hanged_back_to_warehouse" model="ir.ui.view">
            <field name="name">hanged.stock.quant.back.to.warehouseform</field>
            <field name="model">wizard_hanged_back_to_warehouse</field>
            <field name="arch" type="xml">
                <form string="Stock Quant Wizard">
                    <sheet>
                        <group>
                            <group>
                                <field name="partner_id" />
                                <field name="warehouse_id"/>
                                <field name="location_id"/>
                            </group>
                            <group>
                                <field name="picking_type_id" />
                                <field name="location_dest_id" />
                                <field name="scan_option"  required="True"/>
                            </group>
                        </group>
                        <separator string="Selected Lines"/>
                            <field name="stk_quant_ids">
                                <tree editable="bottom" create="false">
                                    <field name="product_id" readonly="True"/>
                                    <field name="quantity" readonly="True"/>
                                    <field name="quantity_moved"/>
                                </tree>
                            </field>
                         <footer>
                        <button name="do_transfer_selected_lines" type='object' string='Confirm'
                                class=" oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_wizard_hanged_back_to_warehouse_quant" model="ir.actions.act_window">
            <field name="name">Create Stock Picking Back To Warehouse</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard_hanged_back_to_warehouse</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>


    </data>
</odoo>